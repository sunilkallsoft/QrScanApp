<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".views.auth.login.LoginFragment"
    android:clickable="true"
    android:focusable="true"
    android:fillViewport="true"
    >
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        >

        <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/backBtn"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintDimensionRatio="W, 1:1"
            android:src="@drawable/ic_back"
            app:layout_constraintWidth_percent="0.05"
            android:layout_margin="@dimen/_20sdp"
            />


        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/viewBlock1"
            android:visibility="gone"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintDimensionRatio="W, 1:1"
            app:layout_constraintHeight_percent="0.6"
            android:background="@color/white"/>

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:id="@+id/cgLogoSeparator"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@id/cgLogo"
            app:layout_constraintBottom_toBottomOf="@id/cgLogo"/>
        <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/cmLogo"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/cgLogoSeparator"
            app:layout_constraintDimensionRatio="W, 1:1"
            android:src="@drawable/img_cm"
            app:layout_constraintWidth_percent="0.5"
            android:layout_marginEnd="-30dp"
            />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/cgLogo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/cmLogo"
            app:layout_constraintTop_toBottomOf="@id/backBtn"
            app:layout_constraintDimensionRatio="W, 1:1"
            android:src="@drawable/img_cg"
            app:layout_constraintWidth_percent="0.2"
            />



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/loginLabel"
            app:layout_constraintTop_toBottomOf="@id/cgLogo"
            app:layout_constraintVertical_bias="0.02"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/cmLogo"
            android:textColor="@color/black"
            android:textSize="@dimen/txt_30sdp"
            android:text="@string/e_pravesh_suvidha"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/registrationLabel"
            app:layout_constraintTop_toBottomOf="@id/cmLogo"
            app:layout_constraintVertical_bias="0.1"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/black"
            android:textSize="@dimen/txt_30sdp"
            android:text="@string/cm_gate_pass_form"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/visitorNameLabel"
            app:layout_constraintTop_toBottomOf="@id/registrationLabel"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/visitor_s_name"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/nameInputLayout"
            app:layout_constraintTop_toBottomOf="@id/visitorNameLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            app:boxBackgroundColor="@color/white"
            app:hintEnabled="false"
            app:boxStrokeColor="@color/grey"
            app:boxCornerRadiusTopStart="@dimen/card_corner_radius"
            app:boxCornerRadiusTopEnd="@dimen/card_corner_radius"
            app:boxCornerRadiusBottomStart="@dimen/card_corner_radius"
            app:boxCornerRadiusBottomEnd="@dimen/card_corner_radius"
            style="@style/Widget.Material3.TextInputLayout.OutlinedBox">
            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/nameInput"
                android:hint="@string/name"
                android:inputType="text"
                android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZ"
                android:textAllCaps="true"
                android:textSize="@dimen/txt_14sdp"/>
        </com.google.android.material.textfield.TextInputLayout>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/fathersNameLabel"
            app:layout_constraintTop_toBottomOf="@id/nameInputLayout"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/father_s_name"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/fathersNameInputLayout"
            app:layout_constraintTop_toBottomOf="@id/fathersNameLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            app:boxBackgroundColor="@color/white"
            app:hintEnabled="false"
            app:boxStrokeColor="@color/grey"
            app:boxCornerRadiusTopStart="@dimen/card_corner_radius"
            app:boxCornerRadiusTopEnd="@dimen/card_corner_radius"
            app:boxCornerRadiusBottomStart="@dimen/card_corner_radius"
            app:boxCornerRadiusBottomEnd="@dimen/card_corner_radius"
            style="@style/Widget.Material3.TextInputLayout.OutlinedBox">
            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/fathersNameInput"
                android:hint="@string/father_s_name"
                android:inputType="text"
                android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZ"
                android:textAllCaps="true"
                android:textSize="@dimen/txt_14sdp"/>
        </com.google.android.material.textfield.TextInputLayout>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/genderLabel"
            app:layout_constraintTop_toBottomOf="@id/fathersNameInputLayout"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/gender"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <RadioGroup
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/genderRadioContainer"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@id/genderLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            android:background="@drawable/curved_rectangle_white_bg_16dp"
            >
            <RadioButton
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/maleRadio"
                android:text="@string/male"
                android:layout_weight="1"
                android:checked="true"
                android:layout_marginVertical="@dimen/_5sdp"
                android:layout_marginHorizontal="@dimen/_10sdp"/>

            <RadioButton
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/femaleRadio"
                android:text="@string/female"
                android:layout_weight="1"
                android:layout_marginVertical="@dimen/_5sdp"
                android:layout_marginHorizontal="@dimen/_10sdp"/>

            <RadioButton
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/otherRadio"
                android:text="@string/other"
                android:layout_weight="1"
                android:layout_marginVertical="@dimen/_5sdp"
                android:layout_marginHorizontal="@dimen/_10sdp"/>
        </RadioGroup>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/visitorsAgeLabel"
            app:layout_constraintTop_toBottomOf="@id/genderRadioContainer"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/age_label"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/visitorsAgeInputLayout"
            app:layout_constraintTop_toBottomOf="@id/visitorsAgeLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            app:boxBackgroundColor="@color/white"
            app:hintEnabled="false"
            app:boxStrokeColor="@color/grey"
            app:boxCornerRadiusTopStart="@dimen/card_corner_radius"
            app:boxCornerRadiusTopEnd="@dimen/card_corner_radius"
            app:boxCornerRadiusBottomStart="@dimen/card_corner_radius"
            app:boxCornerRadiusBottomEnd="@dimen/card_corner_radius"
            style="@style/Widget.Material3.TextInputLayout.OutlinedBox">
            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/visitorsAgeInput"
                android:hint="@string/age"
                android:inputType="number"
                android:digits="0123456789"
                android:textSize="@dimen/txt_14sdp"/>
        </com.google.android.material.textfield.TextInputLayout>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mobileNumberLabel"
            app:layout_constraintTop_toBottomOf="@id/visitorsAgeInputLayout"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/visitor_s_mobile_no"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/mobileNoInputLayout"
            app:layout_constraintTop_toBottomOf="@id/mobileNumberLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            app:boxBackgroundColor="@color/white"
            app:hintEnabled="false"
            app:boxStrokeColor="@color/grey"
            app:boxCornerRadiusTopStart="@dimen/card_corner_radius"
            app:boxCornerRadiusTopEnd="@dimen/card_corner_radius"
            app:boxCornerRadiusBottomStart="@dimen/card_corner_radius"
            app:boxCornerRadiusBottomEnd="@dimen/card_corner_radius"
            style="@style/Widget.Material3.TextInputLayout.OutlinedBox">
            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/mobileNoInput"
                android:hint="@string/_10_digits_mobile_number"
                android:inputType="phone"
                android:digits="0123456789"
                android:maxLength="10"
                android:focusable="false"
                android:clickable="false"
                android:textSize="@dimen/txt_14sdp"/>
        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/objectiveOfMeetLabel"
            app:layout_constraintTop_toBottomOf="@id/mobileNoInputLayout"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/objective_of_meet"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/objectiveInputLayout"
            app:layout_constraintTop_toBottomOf="@id/objectiveOfMeetLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            app:boxBackgroundColor="@color/white"
            app:hintEnabled="false"
            app:boxStrokeColor="@color/grey"
            app:boxCornerRadiusTopStart="@dimen/card_corner_radius"
            app:boxCornerRadiusTopEnd="@dimen/card_corner_radius"
            app:boxCornerRadiusBottomStart="@dimen/card_corner_radius"
            app:boxCornerRadiusBottomEnd="@dimen/card_corner_radius"
            style="@style/Widget.Material3.TextInputLayout.OutlinedBox">
            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/objectiveInput"
                android:inputType="textMultiLine"
                android:digits="0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHUJKLMNOPQRSTUVWXYZ "
                android:minLines="3"
                android:maxLines="5"
                android:textSize="@dimen/txt_14sdp"
                android:gravity="start|top"/>
        </com.google.android.material.textfield.TextInputLayout>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/meetDateLabel"
            app:layout_constraintTop_toBottomOf="@id/objectiveInputLayout"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/meet_date"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/meetDateLayout"
            app:layout_constraintTop_toBottomOf="@id/meetDateLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            app:boxBackgroundColor="@color/white"
            app:hintEnabled="false"
            app:boxStrokeColor="@color/grey"
            app:boxCornerRadiusTopStart="@dimen/card_corner_radius"
            app:boxCornerRadiusTopEnd="@dimen/card_corner_radius"
            app:boxCornerRadiusBottomStart="@dimen/card_corner_radius"
            app:boxCornerRadiusBottomEnd="@dimen/card_corner_radius"
            style="@style/Widget.Material3.TextInputLayout.OutlinedBox">
            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/meetDateInput"
                android:inputType="date"
                android:hint="dd-mm-yyyy"
                android:drawableEnd="@drawable/ic_dropdown"
                android:focusable="false"
                android:clickable="false"
                android:textSize="@dimen/txt_14sdp"/>
        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/assemblyAreaLabel"
            app:layout_constraintTop_toBottomOf="@id/meetDateLayout"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/assembly_area"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <AutoCompleteTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/assemblyArea"
            app:layout_constraintTop_toBottomOf="@id/assemblyAreaLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginHorizontal="@dimen/_20sdp"
            android:layout_marginVertical="@dimen/_5sdp"
            android:padding="@dimen/card_corner_radius"
            android:drawableEnd="@drawable/ic_dropdown"
            android:textSize="@dimen/txt_16sdp"
            android:elevation="@dimen/_2sdp"
            android:focusable="false"
            android:clickable="false"
            android:background="@drawable/curved_rectangle_white_bg_16dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/districtLabel"
            app:layout_constraintTop_toBottomOf="@id/assemblyArea"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/district"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <AutoCompleteTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/district"
            app:layout_constraintTop_toBottomOf="@id/districtLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            android:padding="@dimen/card_corner_radius"
            android:drawableEnd="@drawable/ic_dropdown"
            android:textSize="@dimen/txt_16sdp"
            android:elevation="@dimen/_2sdp"
            android:focusable="false"
            android:clickable="false"
            android:background="@drawable/curved_rectangle_white_bg_16dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/urbanRuralLabel"
            app:layout_constraintTop_toBottomOf="@id/district"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/urban_rural"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <AutoCompleteTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/urbanRural"
            app:layout_constraintTop_toBottomOf="@id/urbanRuralLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            android:padding="@dimen/card_corner_radius"
            android:drawableEnd="@drawable/ic_dropdown"
            android:textSize="@dimen/txt_16sdp"
            android:elevation="@dimen/_2sdp"
            android:focusable="false"
            android:clickable="false"
            android:background="@drawable/curved_rectangle_white_bg_16dp"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/civicBodiesLabel"
            app:layout_constraintTop_toBottomOf="@id/urbanRural"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/civic_bodies_label"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            android:visibility="gone"
            />

        <AutoCompleteTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/civicBodies"
            app:layout_constraintTop_toBottomOf="@id/civicBodiesLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            android:padding="@dimen/card_corner_radius"
            android:visibility="gone"
            android:drawableEnd="@drawable/ic_dropdown"
            android:textSize="@dimen/txt_16sdp"
            android:elevation="@dimen/_2sdp"
            android:focusable="false"
            android:clickable="false"
            android:background="@drawable/curved_rectangle_white_bg_16dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/wardLabel"
            app:layout_constraintTop_toBottomOf="@id/civicBodies"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:visibility="gone"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/ward_label"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <AutoCompleteTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/ward"
            android:visibility="gone"
            app:layout_constraintTop_toBottomOf="@id/wardLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            android:padding="@dimen/card_corner_radius"
            android:drawableEnd="@drawable/ic_dropdown"
            android:textSize="@dimen/txt_16sdp"
            android:elevation="@dimen/_2sdp"
            android:focusable="false"
            android:clickable="false"
            android:background="@drawable/curved_rectangle_white_bg_16dp"/>



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/blockLabel"
            app:layout_constraintTop_toBottomOf="@id/urbanRural"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/block_label"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            android:visibility="gone"
            />

        <AutoCompleteTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/block"
            app:layout_constraintTop_toBottomOf="@id/blockLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            android:padding="@dimen/card_corner_radius"
            android:visibility="gone"
            android:drawableEnd="@drawable/ic_dropdown"
            android:textSize="@dimen/txt_16sdp"
            android:elevation="@dimen/_2sdp"
            android:focusable="false"
            android:clickable="false"
            android:background="@drawable/curved_rectangle_white_bg_16dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/panchayatLabel"
            app:layout_constraintTop_toBottomOf="@id/block"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:visibility="gone"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/panchayat_label"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <AutoCompleteTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/panchayat"
            android:visibility="gone"
            app:layout_constraintTop_toBottomOf="@id/panchayatLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            android:padding="@dimen/card_corner_radius"
            android:drawableEnd="@drawable/ic_dropdown"
            android:textSize="@dimen/txt_16sdp"
            android:elevation="@dimen/_2sdp"
            android:focusable="false"
            android:clickable="false"
            android:background="@drawable/curved_rectangle_white_bg_16dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/villageLabel"
            app:layout_constraintTop_toBottomOf="@id/panchayat"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:visibility="gone"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/village_label"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <AutoCompleteTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/village"
            android:visibility="gone"
            app:layout_constraintTop_toBottomOf="@id/villageLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            android:padding="@dimen/card_corner_radius"
            android:drawableEnd="@drawable/ic_dropdown"
            android:textSize="@dimen/txt_16sdp"
            android:elevation="@dimen/_2sdp"
            android:focusable="false"
            android:clickable="false"
            android:background="@drawable/curved_rectangle_white_bg_16dp"/>

        <androidx.constraintlayout.widget.Barrier
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/civicBodiesBarrier"
            app:barrierDirection="bottom"
            app:constraint_referenced_ids="urbanRural, civicBodies, ward, panchayat, village"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/visitorWithNoLabel"
            app:layout_constraintTop_toBottomOf="@id/civicBodiesBarrier"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/grey"
            android:textSize="@dimen/txt_14sdp"
            android:text="@string/no_of_persons_with"
            android:layout_margin="@dimen/_20sdp"
            android:paddingHorizontal="@dimen/_5sdp"
            />

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/visitorWithNoInputLayout"
            app:layout_constraintTop_toBottomOf="@id/visitorWithNoLabel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginVertical="@dimen/_5sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            app:boxBackgroundColor="@color/white"
            app:hintEnabled="false"
            app:boxStrokeColor="@color/grey"
            app:boxCornerRadiusTopStart="@dimen/card_corner_radius"
            app:boxCornerRadiusTopEnd="@dimen/card_corner_radius"
            app:boxCornerRadiusBottomStart="@dimen/card_corner_radius"
            app:boxCornerRadiusBottomEnd="@dimen/card_corner_radius"
            style="@style/Widget.Material3.TextInputLayout.OutlinedBox">
            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/visitorWithNoInput"
                android:inputType="number"
                android:digits="0123456789"
                android:maxLength="3"
                android:textSize="@dimen/txt_14sdp"/>
        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            android:id="@+id/registerBtn"
            android:textColor="@color/white"
            android:text="@string/apply_label"
            android:textStyle="bold"
            android:gravity="center"
            android:layout_marginVertical="@dimen/_20sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            app:layout_constraintTop_toBottomOf="@id/visitorWithNoInputLayout"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:padding="@dimen/card_corner_radius"
            android:background="@drawable/rounded_solid_color_primary_bg"/>

        <ProgressBar
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_30sdp"
            android:id="@+id/btnClickProgress"
            android:visibility="gone"
            android:elevation="@dimen/_2sdp"
            android:indeterminateTint="@color/colorPrimary"
            app:layout_constraintStart_toStartOf="@id/registerBtn"
            app:layout_constraintEnd_toEndOf="@id/registerBtn"
            app:layout_constraintTop_toTopOf="@id/registerBtn"
            app:layout_constraintBottom_toBottomOf="@id/registerBtn"
            />

        <View
            android:layout_width="0dp"
            android:layout_height="@dimen/_30sdp"
            app:layout_constraintTop_toBottomOf="@id/registerBtn"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>